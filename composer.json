{
    "name": "Ibtikar/GlanceDashboardBundle",
    "type": "laravel-library",
    "description": "Ibtikar share economy user management system used mongodb",
    "keywords": ["User management"],
    "homepage": "https://github.com/Ibtikar/GlanceDashboardBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Ibtikar"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "symfony/form": "~3.0",
        "symfony/framework-bundle": "~3.0",
        "symfony/security-bundle": "~2.3|~3.0",
        "symfony/twig-bundle": "~2.3|~3.0",
        "composer/installers": "~1.0",
        "symfony/finder": "^3.1",
        "doctrine/mongodb-odm": "~1.0",
        "doctrine/mongodb-odm-bundle": "~3.0"
    },
    "require-dev": {
        "swiftmailer/swiftmailer": "~4.3|~5",
        "symfony/validator": "~2.3|~3.0",
        "symfony/yaml": "~2.3|~3.0"
    },
    "autoload": {
        "psr-4": {
            "Ibtikar\\GlanceDashboardBundle\\": ""
        }
    },
    "extra": {
        "installer-paths": {
            "src/{$name}": ["Ibtikar/GlanceDashboardBundle"]
        }
    }
}
