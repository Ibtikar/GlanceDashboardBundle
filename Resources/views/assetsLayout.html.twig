{% extends '::base.html.twig' %}

{% block meta %}
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

{% endblock %}


{% block stylesheets %}
    {{ parent() }}
    <!-- Favicons AND TOUCH ICONS   -->
    <link rel="icon" href="{{ asset('bundles/ibtikarglancedashboard/assets/images/'~style_Sheet_name~'/favicon.ico')}}">
    <link rel="apple-touch-icon" href="{{ asset('bundles/ibtikarglancedashboard/assets/images/'~style_Sheet_name~'/apple-touch-icon.png')}}">
    <link rel="apple-touch-icon" sizes="76x76" href="{{ asset('bundles/ibtikarglancedashboard/assets/images/'~style_Sheet_name~'/apple-touch-icon-76x76.png')}}">
    <link rel="apple-touch-icon" sizes="120x120" href="{{ asset('bundles/ibtikarglancedashboard/assets/images/'~style_Sheet_name~'/apple-touch-icon-120x120.png')}}">
    <link rel="apple-touch-icon" sizes="152x152" href="{{ asset('bundles/ibtikarglancedashboard/assets/images/'~style_Sheet_name~'/apple-touch-icon-152x152.png')}}">

    <link href="https://fonts.googleapis.com/css?family=Roboto:400,300,100,500,700,900" rel="stylesheet" type="text/css">



    <!-- Global stylesheets -->
    <link href="{{ asset('bundles/ibtikarglancedashboard/assets/css/icons/icomoon/styles.css')}}" rel="stylesheet" type="text/css">
    <link href="{{ asset('bundles/ibtikarglancedashboard/assets/css/bootstrap.css')}}" rel="stylesheet" type="text/css">
    <link href="{{ asset('bundles/ibtikarglancedashboard/assets/css/core.css')}}" rel="stylesheet" type="text/css">
    <link href="{{ asset('bundles/ibtikarglancedashboard/assets/css/components.css')}}" rel="stylesheet" type="text/css">
    <link href="{{ asset('bundles/ibtikarglancedashboard/assets/css/colors.css')}}" rel="stylesheet" type="text/css">

    <!-- /global stylesheets -->

    <!-- file upload Plugin CSS -->
    <link rel="{{ asset('bundles/ibtikarglancedashboard/stylesheet" href="assets/js/plugins/fileupload/bootstrap-fileupload.css')}}">
        <link href="{{ asset('bundles/ibtikarglancedashboard/assets/css/custom.css')}}" rel="stylesheet" type="text/css">

        <link href="{{ asset('bundles/ibtikarglancedashboard/assets/css/'~style_Sheet_name~'.css')}}" rel="stylesheet" type="text/css">




{% endblock %}

{% block javascripts %}
    {{ parent() }}



    <script type="text/javascript" src="{{ asset('bundles/ibtikarglancedashboard/assets/js/plugins/loaders/pace.min.js')}}"></script>
    <script type="text/javascript" src="{{ asset('bundles/ibtikarglancedashboard/assets/js/core/libraries/jquery.min.js')}}"></script>
    <script type="text/javascript" src="{{ asset('bundles/ibtikarglancedashboard/assets/js/core/libraries/bootstrap.min.js')}}"></script>
    <script type="text/javascript" src="{{ asset('bundles/ibtikarglancedashboard/assets/js/plugins/loaders/blockui.min.js')}}"></script>
    <script type="text/javascript" src="{{ asset('bundles/ibtikarglancedashboard/assets/js/plugins/ui/nicescroll.min.js')}}"></script>
    <script type="text/javascript" src="{{ asset('bundles/ibtikarglancedashboard/assets/js/plugins/ui/drilldown.js')}}"></script>
    <script type="text/javascript" src="{{ asset('bundles/ibtikarglancedashboard/assets/js/pages/layout_sidebar_sticky_custom.js')}}"></script>

    <!-- /core JS files -->

    <!-- alerts messages -->
    <script type="text/javascript" src="{{ asset('bundles/ibtikarglancedashboard/assets/js/plugins/notifications/pnotify.min.js')}}"></script>
    <script type="text/javascript" src="{{ asset('bundles/ibtikarglancedashboard/assets/js/pages/components_notifications_pnotify.js')}}"></script>

    <script src="{{ asset('bundles/ibtikarglancedashboard/assets/js/core/jquery.easing.min.js')}}"></script>


    <script type="text/javascript" src="{{ asset('bundles/ibtikarglancedashboard/assets/js/core/app.js')}}"></script>

{% endblock %}