                            {% elseif is_granted('ROLE_RECIPEPUBLISH_VIEW') or is_granted('ROLE_ADMIN') %}
                            <li><a href="{{ path('ibtikar_glance_dashboard_recipepublish_list_publish_recipe') }}" {% if 'recipe' in app.request.attributes.get('_route') %}class="highlight"{% endif %}><img width="20" height="20" src="{{ asset('bundles/ibtikarshareeconomydashboarddesign/images/nav/content.png')}}" class=" position-left"/> <span>{% trans from 'recipe'%}List Recipe{% endtrans %}</span></a></li>
                            {% elseif is_granted('ROLE_RECIPEDRAFT_VIEW') or is_granted('ROLE_ADMIN') %}
