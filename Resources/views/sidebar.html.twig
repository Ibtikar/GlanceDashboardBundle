                        <li class="navigation-header"><span>الصفحات ١</span> <i class="icon-menu" data-placement="right" title="الصفحات"></i></li>


                            {% if is_granted('ROLE_JOB_VIEW') or is_granted('ROLE_ADMIN')%}
                                <li><a href="{{ path('ibtikar_glance_ums_job_list') }}" {% if 'job' in app.request.attributes.get('_route') %}class="highlight"{% endif %}><img width="20" height="20" src="{{ asset('bundles/ibtikarshareeconomydashboarddesign/images/nav/jobs.png')}}" class=" position-left"/> <span>{% trans %}list Jobs{% endtrans %}</span></a></li>
                                {% endif %}
                                {% if is_granted('ROLE_CITY_VIEW') or is_granted('ROLE_ADMIN')%}
                                <li><a href="{{ path('ibtikar_glance_ums_city_list') }}" {% if 'city' in app.request.attributes.get('_route') %}class="highlight"{% endif %}><img width="20" height="20" src="{{ asset('bundles/ibtikarshareeconomydashboarddesign/images/nav/cities.png')}}" class=" position-left"/> <span>{% trans %}Cities{% endtrans %}</span></a></li>
                                {% endif %}
                                {% if is_granted('ROLE_STAFF_VIEW') or is_granted('ROLE_ADMIN')%}
                                <li><a href="{{ path('ibtikar_glance_ums_staff_list') }}" {% if 'staff' in app.request.attributes.get('_route') and app.request.attributes.get('id') != app.user.id %}class="subActive"{% endif %}><img width="20" height="20" src="{{ asset('bundles/ibtikarshareeconomydashboarddesign/images/nav/workers.png')}}" class=" position-left"/><span>{% trans %}List Staff{% endtrans %}</span></a></li>
                                {% endif %}
                                {% if is_granted('ROLE_VISITOR_VIEW') or is_granted('ROLE_ADMIN')%}
                                <li><a href="{{ path('ibtikar_glance_ums_visitor_list') }}" {% if 'visitor' in app.request.attributes.get('_route') and app.request.attributes.get('id') != app.user.id %}class="subActive"{% endif %}><img width="20" height="20" src="{{ asset('bundles/ibtikarshareeconomydashboarddesign/images/nav/workers.png')}}" class=" position-left"/> <span>{% trans from 'visitor' %}List Visitor{% endtrans %}</span></a></li>
                                {% endif %}
                                {% if is_granted('ROLE_ROLE_VIEW') or is_granted('ROLE_ADMIN')%}
                                <li><a href="{{ path('ibtikar_glance_ums_role_list') }}" {% if 'role' in app.request.attributes.get('_route') %}class="highlight"{% endif %}><img width="20" height="20" src="{{ asset('bundles/ibtikarshareeconomydashboarddesign/images/nav/roles.png')}}" class=" position-left"/> <span>{% trans %}Roles{% endtrans %}</span></a></li>
                                {% endif %}

